# Add docker container as remote interpreter

script path:            wktrees/pso-mpm_api.git/develop/src/manage.py
parameters:             runserver 0.0.0.0:8000
environment variables:  PYTHONUNBUFFERED=1
Python interpreter:     Remote Python 3.9.7 Docker Compose (fmapi_django at [wktrees/pso-mpm_api.git/develop/dev.yml])
working directory:      wktrees/pso-mpm_api.git/develop/src

