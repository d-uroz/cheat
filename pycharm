# Add configuration to run tests in docker container

script path:            wktrees/pso-mpm_api.git/develop/src/manage.py
parameters:             test test_input_validations
environment variables:  PYTHONUNBUFFERED=1;USER_LOGIN=test@mail.com
Python interpreter:     Remote Python 3.9.7 Docker Compose (fmapi_django at [wktrees/pso-mpm_api.git/develop/dev.yml])
working directory:      wktrees/pso-mpm_api.git/develop/src/management/tests/
